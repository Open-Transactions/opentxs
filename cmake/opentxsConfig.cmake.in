@PACKAGE_INIT_STRING@

list(
  APPEND
  CMAKE_MODULE_PATH
  "${CMAKE_CURRENT_LIST_DIR}"
  "@MODULES_DIR_STRING@"
)
include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@.cmake")

set(OT_USE_VCPKG_TARGETS @OT_USE_VCPKG_TARGETS@)
set(OT_STORAGE_SQLITE @OT_STORAGE_SQLITE@)
set(OT_STORAGE_LMDB @OT_STORAGE_LMDB@)
set(OT_CRYPTO_USING_LIBSECP256K1 @OT_CRYPTO_USING_LIBSECP256K1@)
set(OT_BUNDLED_SECP256K1 @OT_BUNDLED_SECP256K1@)
set(OT_WITH_QT @OT_WITH_QT@)
set(OT_WITH_QML @OT_WITH_QML@)
set(OT_USE_PSTL @OT_USE_PSTL@)
set(OT_WITH_TBB @OT_WITH_TBB@)
set(OT_PSTL_NEEDS_TBB @OT_PSTL_NEEDS_TBB@)

if(OT_WITH_QT)
  if(DEFINED QT_VERSION_MAJOR
     AND NOT
         (QT_VERSION_MAJOR
          MATCHES
          @QT_VERSION_MAJOR@)
  )
    message(
      FATAL_ERROR
        "libopentxs is linked against Qt@QT_VERSION_MAJOR@ however ${PROJECT_NAME} is attempting to link to Qt${QT_VERSION_MAJOR}"
    )
  endif()

  set(QT_VERSION_MAJOR @QT_VERSION_MAJOR@)
endif()

include(libopentxs-find-dependencies)
